<div id="results">
</div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Ticker Symbol</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
        <% current_user.stocks.each do |stock| %>
        <tr>
            <th scope="row"><%= stock.ticker %></th>
            <td><%= stock.name %></td>
            <td><%= stock.price %></td>
            <td><%= button_to 'Remove', user_stock_path(stock), method: :delete %></td>
        </tr>
        <% end %>
  </tbody>
</table>